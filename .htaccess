Options +FollowSymLinks
DirectoryIndex page.php
RewriteEngine on


AddDefaultCharset utf-8

RewriteBase /kaktus/
RewriteRule ^(.*)portfolio-(.*)-(.*)\.html$ $1portfolio.php?issue_id=$2
RewriteRule ^(.*)-(.*)-(.*)-(.*)\.html$ $1.php?action=$2&id=$3
RewriteRule ^(.*)-(.*)-(.*)\.html$ $1.php?issue_id=$2
RewriteRule ^(.*)-(.*)\.html$ $1.php?

RewriteRule ^(.*)\.html$ $1.php
